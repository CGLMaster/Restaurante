
package Presentación.Command.TurnoCommands;

import Negocio.FactoriaNegocio.FactoriaSA;
import Presentación.Command.Command;
import Presentación.Command.Context;
import Presentación.Controller.Eventos;

public class BajaTurnoCommand implements Command {
	
	public Context executeCommand(Object datos) {
		int res = FactoriaSA.getInstance().getSATurno().bajaTurno((int) datos);
		Context resContext;
		
		if(res > 0){
			resContext = new Context(Eventos.RES_BAJA_TURNO_OK, datos);
		}else{
			resContext = new Context(Eventos.RES_BAJA_TURNO_KO);
		}
		
		return resContext;
	}
}