package Negocio.Seccion;

public class TSeccion {
	private String zona;
	private int pasillo;
	private int id_seccion;
	private int cont_productos;
	private boolean activo;

	public int getCont_productos() {
		return cont_productos;
	}

	public void setCont_productos(int cont_productos) {
		this.cont_productos = cont_productos;
	}

	public TSeccion(int id_seccion) {
		this.id_seccion = id_seccion;
	}

	public TSeccion(int pasillo, String zona) {
		this.pasillo = pasillo;
		this.zona = zona;
	}

	public String getZona() {
		return zona;
	}

	public int getID_Seccion() {
		return id_seccion;
	}

	public int getPasillo() {
		return pasillo;
	}

	public boolean getActivo() {
		return activo;
	}

	public void setZona(String zona) {
		this.zona = zona;
	}

	public void setPasillo(int pasillo) {
		this.pasillo = pasillo;
	}

	public void setActivo(boolean activo) {
		this.activo = activo;
	}

	public void setId_seccion(int id_seccion) {
		this.id_seccion = id_seccion;
	}

}